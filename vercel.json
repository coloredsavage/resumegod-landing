{
  "version": 2,
  "builds": [
    {
      "src": "api/signup.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/webhook.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "css/**/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "js/**/*.js",
      "use": "@vercel/static"
    },
    {
      "src": "images/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "font/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "font2/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/signup",
      "dest": "/api/signup.js",
      "methods": ["POST", "OPTIONS"]
    },
    {
      "src": "/api/webhook",
      "dest": "/api/webhook.js",
      "methods": ["POST", "OPTIONS"]
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
